  "main": "./build/src/server.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "npm run clean && tsc -p tsconfig.app.json",
    "build:test": "npm run clean && tsc -p tsconfig.spec.json",
    "start": "npm run build && npm run forever-fg",
    "forever-fg": "forever ./build/src/server.js",
    "forever": "forever start -a --uid lynx ./build/src/server.js",
    "forever-stop": "forever stop lynx",
    "watch": "npm run build && concurrently -kr 'tsc --watch' 'nodemon --inspect'",
    "test": "npm run build:test && NODE_ENV=test TS_NODE_PROJECT='./tsconfig.spec.json' nyc mocha"
  },
